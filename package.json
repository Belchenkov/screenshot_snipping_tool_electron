{
  "name": "screenshot_snipping_tool_electron",
  "version": "0.1.0",
  "private": true,
  "author": "Aleksey Belchenkov",
  "dependencies": {
    "@blueprintjs/core": "*",
    "@testing-library/jest-dom": "*",
    "@testing-library/react": "*",
    "@testing-library/user-event": "*",
    "react": "*",
    "react-app-rewired": "*",
    "react-dom": "*",
    "react-router-dom": "*",
    "react-scripts": "*"
  },
  "homepage": ".",
  "scripts": {
    "serve": "react-scripts start",
    "start": "SET DEBUG=true && electron .",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "build": "react-scripts build",
    "build:macos": "electron-builder --macos --dir",
    "build:windows": "electron-builder --windows --dir",
    "build:linux": "electron-builder --linux --dir",
    "publish:github": "build --windows -p always"
  },
  "main": "main.js",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "electron": "*",
    "electron-builder": "^22.6.0"
  }
}
